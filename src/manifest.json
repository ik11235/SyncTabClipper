{
  "name": "syncTabClipper",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "tab clip.",
  "permissions": [
    "tabs",
    "storage",
    "contextMenus"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistant": false
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon.png"
    }
  }
}

