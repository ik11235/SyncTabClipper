{
  "name": "SyncTabClipper",
  "version": "0.2.0",
  "manifest_version": 2,
  "description": "__MSG_Description__",
  "default_locale": "en",
  "permissions": ["tabs", "storage", "contextMenus"],
  "background": {
    "scripts": [
      "js/zlib.js",
      "js/zlib-deflate.js",
      "js/zlib-inflate.js",
      "js/background.js"
    ],
    "persistant": false
  },
  "icons": {
    "16": "images/icon16.png",
    "32": "images/icon32.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "images/icon16.png"
    }
  }
}
